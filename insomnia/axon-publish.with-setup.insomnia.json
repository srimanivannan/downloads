      "preRequestScript": "// Run once to initialize all axon-publish_* variables (and axon_* alias).\nconst keysA = [\n  \"axon-publish_$isoTimestamp\",\n  \"axon-publish_$randomUUID\",\n  \"axon-publish_dummy-url\",\n  \"axon-publish_eps-abu-mdes-topic\",\n  \"axon-publish_eps-bnd-card-update-event-topic\",\n  \"axon-publish_eps-near-real-time-topic\",\n  \"axon-publish_url\"\n];\nconst keysB = [\n  \"axon_$isoTimestamp\",\n  \"axon_$randomUUID\",\n  \"axon_dummy-url\",\n  \"axon_eps-abu-mdes-topic\",\n  \"axon_eps-bnd-card-update-event-topic\",\n  \"axon_eps-near-real-time-topic\",\n  \"axon_url\"\n];\nfor (const k of keysA.concat(keysB)) {\n  if (insomnia.environment.get(k) === undefined) {\n    insomnia.environment.set(k, '');\n  }\n}\nfor (let i=0;i<keysA.length;i++){\n  const a=keysA[i], b=keysB[i];\n  const va=insomnia.environment.get(a);\n  const vb=insomnia.environment.get(b);\n  if ((va||'') && !(vb||'')) insomnia.environment.set(b, va);\n  if ((vb||'') && !(va||'')) insomnia.environment.set(a, vb);\n}\ninsomnia.test('Variables initialized', () => {\n  insomnia.expect(keysA.length >= 0).to.equal(true);\n});",
